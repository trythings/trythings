{
  "name": "ellies-pad",
  "version": "0.0.1",
  "description": "Ellie's Pad",
  "main": "src/index.js",
  "private": true,
  "license": "private",
  "scripts": {
    "production": "NODE_ENV=production npm run build",
    "build": "$npm_package_config_browserify --debug --entry src/index.js --outfile static/index.js",
    "postbuild": "exorcist static/index.js.map < static/index.js > static/temp.js && mv static/temp.js static/index.js",
    "prewatch": "npm run build",
    "watch": "browser-sync start --proxy localhost:8080 --port 3000 --files build --no-open --reload-delay 250 & npm run build --ellies-pad:browserify=watchify"
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "envify",
        {
          "_": "purge"
        }
      ],
      "uglifyify"
    ]
  },
  "config": {
    "browserify": "browserify"
  },
  "repository": {
    "type": "git",
    "url": "github.com/trythings/trythings"
  },
  "author": "Try Things",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-relay-plugin": "^0.6.3",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "exorcist": "^0.4.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-relay": "^0.6.1"
  }
}
